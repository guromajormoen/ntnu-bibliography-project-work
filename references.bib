@article{popovPowderBedFusion2021,
  title = {Powder {{Bed Fusion Additive Manufacturing Using Critical Raw Materials}}: {{A Review}}},
  shorttitle = {Powder {{Bed Fusion Additive Manufacturing Using Critical Raw Materials}}},
  author = {Popov, Vladimir V. and Grilli, Maria Luisa and Koptyug, Andrey and Jaworska, Lucyna and {Katz-Demyanetz}, Alexander and Klob{\v c}ar, Damjan and Balos, Sebastian and Postolnyi, Bogdan O. and Goel, Saurav},
