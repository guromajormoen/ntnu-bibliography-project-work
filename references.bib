  isbn = {978-3-319-95966-5},
  langid = {english},
  keywords = {corrosionCorrosion,Solid Solution Hardener,Super Alloys,Tool wearTool Wear,wearWear},
  file = {/Users/guromajormoen/Zotero/storage/EG8SYM7X/Kishawy og Hosseini - 2019 - Superalloys.pdf}
}

