@article{yuanMCrAlYCoatingDesign2014,
  title = {{{MCrAlY}} Coating Design Based on Oxidation-Diffusion Modelling. {{Part I}}: {{Microstructural}} Evolution},
  shorttitle = {{{MCrAlY}} Coating Design Based on Oxidation-Diffusion Modelling. {{Part I}}},
  author = {Yuan, Kang and Eriksson, Robert and Lin Peng, Ru and Li, Xin-Hai and Johansson, Sten and Wang, Yan-Dong},
  year = 2014,
  month = sep,
  journal = {Surface and Coatings Technology},
  volume = {254},
  pages = {79--96},
  issn = {0257-8972},
  doi = {10.1016/j.surfcoat.2014.05.067},
  urldate = {2025-11-04},
  abstract = {To improve the efficiency of modern gas turbines, it is highly desired to develop durable MCrAlY alloys, to be used as protective coatings against oxidation and corrosion for superalloys which are the base materials for some hot components like turbine blades and vanes. In this paper, an oxidation-diffusion model was used to simulate the diffusion of alloying elements and the corresponding microstructural changes in different superalloy-coating systems at high temperature. Two important processes are considered in this model: oxidation of the coating and interdiffusion between the superalloy and the coating. The model showed an accurate predictability of the diffusion and microstructural evolution in real superalloy-coating diffusion couples studied at high-temperature exposure. The model was further applied to investigate the elemental effects of Ni, Co, Cr and Al on the microstructural evolution, considering the development of two important phases in superalloys and coatings, i.e. FCC-{$\gamma\prime$} and BCC-{$\beta$}, at different temperatures. The results in this paper deepen the knowledge of the MCrAlY coating design for superalloy-coating systems in high-temperature applications.},
  keywords = {Coating design,Interdiffusion,MCrAlY,Microstructural evolution,Oxidation,Superalloy},
  file = {/Users/guromajormoen/Zotero/storage/NR7RSK8A/Yuan et al. - 2014 - MCrAlY coating design based on oxidation-diffusion modelling. Part I Microstructural evolution.pdf;/Users/guromajormoen/Zotero/storage/VFYINLNR/S0257897214004976.html}
}

@article{zhangTemperatureDependentMisfitStress2020,
  title = {Temperature-{{Dependent Misfit Stress}} in {{Gamma Double Prime Strengthened Ni-Base Superalloys}}},
  author = {Zhang, R. Y. and Qin, H. L. and Bi, Z. N. and Li, J. and Paul, S. and Lee, T. L. and Zhang, S. Y. and Zhang, J. and Dong, H. B.},
  year = 2020,
  month = apr,
  journal = {Metallurgical and Materials Transactions A},
  volume = {51},
  number = {4},
  pages = {1860--1873},
  issn = {1543-1940},
  doi = {10.1007/s11661-020-05627-1},
  urldate = {2025-10-20},
  abstract = {Misfit stresses in the {$\gamma{''}$}-strengthened Ni-base superalloy Inconel 718 were calculated from the measured constrained misfit strain using Eshelby's inclusion method. The constrained misfit strains of the {$\gamma{''}$} precipitates were measured using neutron diffraction at various temperatures with the aid of the stress-induced variant selection method. Eshelby tensor was calculated using the expressions for the case of anisotropic matrix given by Mura. Results show the presence of significant compressive misfit stresses in the {$\gamma{''}$} precipitates with an anisotropic distribution, namely 3.0 GPa along the habit plane and 1.7 GPa along the plane normal direction at room temperature, and 2.0 and 1.2 GPa at 664~{$^\circ$}C. The decrease in misfit stresses was due to the decrease in stiffness and the different coefficients of thermal expansion of the {$\gamma$} and {$\gamma{''}$} phases. The average internal stresses in the {$\gamma$} matrix due to lattice misfit were found to be \textasciitilde\,329~MPa at room temperature and \textasciitilde\,186~MPa at 664 {$^\circ$}C in tension. The possibility of relieving such high levels of misfit stresses in precipitates by loss of coherency during continued growth of precipitates is also discussed.},
  langid = {english},
  file = {/Users/guromajormoen/Zotero/storage/VIJ4PXP6/Zhang et al. - 2020 - Temperature-Dependent Misfit Stress in Gamma Double Prime Strengthened Ni-Base Superalloys.pdf}
}
